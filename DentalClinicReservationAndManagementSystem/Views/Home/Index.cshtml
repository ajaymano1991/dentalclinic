<section id="appointment-section" class="ftco-section ftco-no-pt ftco-no-pb">
    <div class="container-fluid px-md-0">
        <div class="row no-gutters">
            <div class="col-md-3 d-flex align-items-stretch">
                <div class="consultation w-100 text-center px-4 px-md-5">
                    <h3 class="mb-4">Dental Services</h3>
                    <p>A small river named Duden flows by their place and supplies</p>
                    <a href="#" class="btn-custom">See Services</a>
                </div>
            </div>
            <div class="col-md-6 d-flex align-items-stretch ">
                <div class="consultation consul w-100 px-4 px-md-5">
                    <div class="text-center">
                        <h3 class="mb-4">Make An Appoinment</h3>
                    </div>
                    @model DentalClinicReservationAndManagementSystem.Models.Appointment
                    @using DentalClinicReservationAndManagementSystem.Models
                    @using (Html.BeginForm("TakeAppointment", "Home", FormMethod.Post, new { @class = "appointment-form well well-lg" }))
                    {
                        @Html.AntiForgeryToken()

                        <div class="row">
                            <!-- Patient Name -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label">Full Name</label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                        @Html.TextBoxFor(model => model.PatientName, new { @class = "form-control", @placeholder = "Enter your full name", required = "required" })
                                    </div>
                                </div>
                            </div>

                            <!-- Phone Number -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label">Mobile Number</label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
                                        @Html.TextBoxFor(model => model.PhoneNumber, new { @class = "form-control", @placeholder = "Enter your phone number", required = "required" })
                                    </div>
                                </div>
                            </div>

                            <!-- Email -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label">Email</label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                                        @Html.TextBoxFor(model => model.Email, new { @class = "form-control", @placeholder = "Enter your email", required = "required" })
                                    </div>
                                </div>
                            </div>

                            <!-- Reason for Appointment -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label">Reason for Appointment</label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                                        @Html.DropDownListFor(model => model.ReasonForAppointment, new SelectList(new[]
                                        {
                                            "ROUTINE DENTAL CHECKUP",
                                            "ROOT CANAL THERAPY",
                                            "DENTAL EXTRACTION AND IMPACTION",
                                            "CROWNS AND BRIDGE",
                                            "DENTURES",
                                            "DENTAL IMPLANTS",
                                            "DIGITAL SMILE DESIGNING",
                                            "FULL MOUTH REHABILITATION",
                                            "GUM DISEASE TREATMENT",
                                            "LASER TREATMENT",
                                            "ORTHODONTIC – BRACES",
                                            "CLEAR ALIGNERS",
                                            "PEDIATRIC DENTISTRY",
                                            "OTHERS"
                                        }), "Select a reason", new { @class = "form-control", required = "required", style = "color: black;" })
                                    </div>
                                </div>
                            </div>

                            <!-- Preferred Date & Time -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label">Preferred Date & Time</label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                                        @Html.TextBoxFor(model => model.PreferredDateTime, new { @class = "form-control", @type = "datetime-local", required = "required" })
                                    </div>
                                </div>
                            </div>

                            <!-- Submit Button -->
                            <div class="col-md-12 text-center">
                                <button type="submit" class="btn btn-primary btn-lg">
                                    <i class="glyphicon glyphicon-check"></i> Make Appointment
                                </button>
                            </div>
                        </div>
                    }

                </div>
            </div>
            <div class="col-md-3 d-flex align-items-stretch">
                <div class="consultation w-100 text-center px-4 px-md-5">
                    <h3 class="mb-4">Find A Doctor</h3>
                    <p>A small river named Duden flows by their place and supplies</p>
                    <a href="#" class="btn-custom">Mee our doctor</a>
                </div>
            </div>
        </div>
    </div>
</section>
