<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,500,600,700" rel="stylesheet">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/open-iconic-bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/animate.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/owl.carousel.min.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/owl.theme.default.min.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/magnific-popup.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/aos.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/ionicons.min.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/bootstrap-datepicker.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/jquery.timepicker.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/flaticon.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/icomoon.css")">
    <link rel="stylesheet" href="@Url.Content("../../Content/css/style.css")">
    <style>
        @@keyframes scroll-left {
            0% {
                transform: translateX(100%);
                animation-timing-function: ease-in; /* Fast start */
            }

            5% {
                transform: translateX(0%); /* Reach normal position in 2s */
                animation-timing-function: linear;
            }

            100% {
                transform: translateX(-100%);
                animation-timing-function: linear; /* Slow continuous scroll */
            }
        }

        .scrolling-wrapper {
            overflow: hidden;
            background-color: #004aad;
            color: white;
            padding: 10px 0;
            white-space: nowrap;
            font-weight: bold;
            font-size: 16px;
            position: relative;
        }

        .scrolling-content {
            display: inline-block;
            padding-left: 100%;
            animation: scroll-left 32s ease-in-out infinite; /* 2s fast + 30s slow */
        }




    </style>



</head>
<body>
    <div class="scrolling-wrapper">
        <div class="scrolling-content">
            📢 Book your appointment today! | Get a free consultation for new patients! | Call us at +123 456 7890
        </div>
    </div>


    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <div class="text-left mt-2 logo">
                <a href="#home">
                    <img src="~/Content/images/logo3.png" alt="Logo" class="img-fluid" style="max-height: 200px;">
                </a>
            </div>
            <div class="text-left logo2" style="height: 80px;position:relative;top:-40px">
                <a href="#home">
                    <img src="~/Content/images/logo5.jpeg" alt="Logo" class="img-fluid" style="height: 130px;width:150px;">
                </a>
            </div>
            

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item nav-link"> @Html.ActionLink("Home", "Index", "Home", new { Style = "color:White" })</li>
                    <li class="nav-item nav-link"> @Html.ActionLink("About", "About", "Home", new { Style = "color:White" })</li>
                    <li class="nav-item nav-link"> @Html.ActionLink("Services", "Services", "Home", new { Style = "color:White" })</li>
                    <li class="nav-item nav-link"> @Html.ActionLink("Dentists", "Dentists", "Home", new { Style = "color:White" })</li>
                    <li class="nav-item cta nav-link"> @Html.ActionLink("Make an Appointment", "TakeAppointment", "Home", new { Style = "color:White" })</li>
                    <li class="nav-item cta nav-link"> @Html.ActionLink("Dentist Arena", "DentistLogin", "Home", new { Style = "color:White" })</li>
                    <li class="nav-item cta nav-link"> @Html.ActionLink("Patient Arena", "PatientLogin", "Home", new { Style = "color:White" })</li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- END nav -->

    <section class="home-slider owl-carousel">
        <div class="slider-item" style="background-image: url('../../Content/images/bg_2.jpeg');">
            <div class="overlay"></div>
            <div class="container">
                <div class="row slider-text align-items-center" data-scrollax-parent="true">
                    <div class="col-md-6 col-sm-12 ftco-animate" data-scrollax=" properties: { translateY: '70%' }">
                        @*<h1 class="mb-4" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">Modern Dentistry in a Calm and Relaxed Environment</h1>
                            <p class="mb-4" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">A small river named Duden flows by their place and supplies it with the necessary regelialia.</p>*@

                    </div>
                </div>
            </div>
        </div>

        <div class="slider-item" style="background-image: url('../../Content/images/bg_3.jpeg');">
            <div class="overlay"></div>
            <div class="container">
                <div class="row slider-text align-items-center" data-scrollax-parent="true">
                    <div class="col-md-6 col-sm-12 ftco-animate" data-scrollax=" properties: { translateY: '70%' }">
                        @*<h1 class="mb-4" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">Modern Achieve Your Desired Perfect Smile</h1>
                            <p class="mb-4">A small river named Duden flows by their place and supplies it with the necessary regelialia.</p>*@

                    </div>
                </div>
            </div>
        </div>
        <div class="slider-item" style="background-image: url('../../Content/images/bg_1.jpeg');">
            <div class="overlay"></div>
            <div class="container">
                <div class="row slider-text align-items-center" data-scrollax-parent="true">
                    <div class="col-md-6 col-sm-12 ftco-animate" data-scrollax=" properties: { translateY: '70%' }">
                        @*<h1 class="mb-4" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">Modern Achieve Your Desired Perfect Smile</h1>
                            <p class="mb-4">A small river named Duden flows by their place and supplies it with the necessary regelialia.</p>*@

                    </div>
                </div>
            </div>
        </div>
    </section>


    <div>@RenderBody()</div>


    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>

    <!-- Modal -->
    <!--Modal: Login / Register Form-->
    <div class="modal fade" id="modalRequest" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog cascading-modal" role="document">
            <!--Content-->
            <div class="modal-content">

                <!--Modal cascading tabs-->
                <div class="modal-c-tabs">

                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs md-tabs tabs-2 light-blue darken-3" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" data-toggle="tab" href="#panel7" role="tab">
                                <i class="fas fa-user mr-1"></i>
                                Login
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#panel8" role="tab">
                                <i class="fas fa-user-plus mr-1"></i>
                                Register
                            </a>
                        </li>
                    </ul>

                    <!-- Tab panels -->
                    <div class="tab-content">
                        <!--Panel 7-->
                        <div class="tab-pane fade in show active" id="panel7" role="tabpanel">

                            <!--Body-->
                            <div class="modal-body mb-1">
                                <div class="md-form form-sm mb-5">
                                    <i class="fas fa-envelope prefix"></i>
                                    <input type="email" id="modalLRInput10" class="form-control form-control-sm validate">
                                    <label data-error="wrong" data-success="right" for="modalLRInput10">Your email</label>
                                </div>

                                <div class="md-form form-sm mb-4">
                                    <i class="fas fa-lock prefix"></i>
                                    <input type="password" id="modalLRInput11" class="form-control form-control-sm validate">
                                    <label data-error="wrong" data-success="right" for="modalLRInput11">Your password</label>
                                </div>
                                <div class="text-center mt-2">
                                    <button class="btn btn-info">Log in <i class="fas fa-sign-in ml-1"></i></button>
                                </div>
                            </div>
                            <!--Footer-->
                            <div class="modal-footer">
                                <div class="options text-center text-md-right mt-1">
                                    <p>Not a member? <a href="#" class="blue-text">Sign Up</a></p>
                                    <p>Forgot <a href="#" class="blue-text">Password?</a></p>
                                </div>
                                <button type="button" class="btn btn-outline-info waves-effect ml-auto" data-dismiss="modal">Close</button>
                            </div>

                        </div>
                        <!--/.Panel 7-->
                        <!--Panel 8-->
                        <div class="tab-pane fade" id="panel8" role="tabpanel">

                            <!--Body-->
                            <div class="modal-body">
                                <div class="md-form form-sm mb-5">
                                    <i class="fas fa-envelope prefix"></i>
                                    <input type="email" id="modalLRInput12" class="form-control form-control-sm validate">
                                    <label data-error="wrong" data-success="right" for="modalLRInput12">Your email</label>
                                </div>

                                <div class="md-form form-sm mb-5">
                                    <i class="fas fa-lock prefix"></i>
                                    <input type="password" id="modalLRInput13" class="form-control form-control-sm validate">
                                    <label data-error="wrong" data-success="right" for="modalLRInput13">Your password</label>
                                </div>

                                <div class="md-form form-sm mb-4">
                                    <i class="fas fa-lock prefix"></i>
                                    <input type="password" id="modalLRInput14" class="form-control form-control-sm validate">
                                    <label data-error="wrong" data-success="right" for="modalLRInput14">Repeat password</label>
                                </div>

                                <div class="text-center form-sm mt-2">
                                    <button class="btn btn-info">Sign up <i class="fas fa-sign-in ml-1"></i></button>
                                </div>

                            </div>
                            <!--Footer-->
                            <div class="modal-footer">
                                <div class="options text-right">
                                    <p class="pt-1">Already have an account? <a href="#" class="blue-text">Log In</a></p>
                                </div>
                                <button type="button" class="btn btn-outline-info waves-effect ml-auto" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                        <!--/.Panel 8-->
                    </div>

                </div>
            </div>
            <!--/.Content-->
        </div>
    </div>



    <!--End Nav-->
    @RenderSection("Scripts")
    <script src="../../Content/js/jquery.min.js"></script>
    <script src="../../Content/js/jquery-migrate-3.0.1.min.js"></script>
