    <script>
        $(document).ready(function () {
            // Smooth scrolling for same-page links
            $("a[href^='#']").on("click", function (e) {
                e.preventDefault();
                var target = $($(this).attr("href"));

                if (target.length) {
                    $("html, body").animate({
                        scrollTop: target.offset().top - 50 // Adjust based on navbar height
                    }, 1200, "easeInOutExpo"); // Smooth easing effect
                }
            });

            // Smooth scrolling after page reload if hash exists
            if (window.location.hash) {
                var target = $(window.location.hash);
                if (target.length) {
                    $("html, body").animate({
                        scrollTop: target.offset().top - 50
                    }, 1200, "easeInOutExpo");
                }
            }
        });
    </script>

    <script>

        function showAlert(message, type) {
            const alertDiv = document.createElement('div');
            alertDiv.className = `alert alert-${type} alert-dismissible fade show shadow`;
            alertDiv.setAttribute("role", "alert");

            // Style for bottom-center positioning
            alertDiv.style.position = 'relative';
            alertDiv.style.bottom = '20px';
            alertDiv.style.left = '50%';
            alertDiv.style.transform = 'translateX(-50%)';
            alertDiv.style.zIndex = '1055';
            alertDiv.style.maxWidth = '90%';
            alertDiv.style.minWidth = '300px';
            alertDiv.style.textAlign = 'center';

            alertDiv.innerHTML = `
                            <strong>${type.charAt(0).toUpperCase() + type.slice(1)}:</strong> ${message}
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                            `;

            document.body.appendChild(alertDiv);

            // Auto-dismiss after 10 seconds
            setTimeout(() => {
                alertDiv.classList.remove("show");
                alertDiv.classList.add("hide");
                setTimeout(() => alertDiv.remove(), 500); // Wait for fade out
            }, 6000);
        }
    </script>






